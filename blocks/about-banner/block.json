{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "theme/about-banner",
    "title": "About Banner",
    "category": "theme",
    "description": "Responsive hero section with gradient background",
    "textdomain": "your-theme-textdomain", 
    "attributes": {
      "gradientStart": {
        "type": "string",
        "default": "#009bbe"
      }
      "gradientEnd": {
        "type": "string",
        "default": "#0061ff"
      },
      "title": {
        "type": "string",
        "default": "Who We Are"
      },
      "content": {
        "type": "string",
        "source": "html",
        "selector": ".banner-content p",
        "default": ""
      },
      "mediaId": {
        "type": "number",
        "default": 0 
      },
      "mediaUrl": {
        "type": "string",
        "default": "" 
      },
      "mediaAlt": {
        "type": "string",
        "source": "attribute",
        "selector": ".banner-image img",
        "attribute": "alt",
        "default": "" 
      }
    },
    "editorScript": "file:./edit.js",
    "script": "file:./view.js", 
    "style": "file:./style.css",
    "render": "file:./render.php",
    "supports": {
      "align": ["full"],
      "anchor": true,
      "customClassName": false,
      "html": false
    },